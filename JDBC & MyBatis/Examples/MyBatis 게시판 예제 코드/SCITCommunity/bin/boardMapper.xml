<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 회원정보 처리 SQL -->
<mapper namespace="dao.BoardMapper">
	<select id="noticeList" resultType="Board">
		SELECT
			*
		FROM
			notice_board
		ORDER BY
			num DESC
	</select>
	
	<select id="readNotice" parameterType="int" resultType="Board">
		SELECT
			*
		FROM
			notice_board
		WHERE
			num = #{num}
	</select>
	
	<select id="freeList" resultType="Board">
		SELECT
			*
		FROM
			free_board
		ORDER BY
			num DESC
	</select>
	
	<select id="readFree" parameterType="int" resultType="Board">
		SELECT
			*
		FROM
			free_board
		WHERE
			num = #{num}
	</select>
</mapper>